{
  "manifest_version": 3,
  "name": "Tab Profiles",
  "version": "1.0",
  "description": "Switch among named tab-sets and restore your original tabs.",
  "permissions": ["tabs", "storage"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "48": "48.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "48.png"
  }
}
